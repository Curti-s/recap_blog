<div id="post_show_content" class="skinny_wrapper wrapper_padding">
      <header>
        <p class="date"><%= @project.created_at.strftime('%A %b %d')%></p>
        <h1><%= @project.title %></h1>
        <hr>
      </header>
      <div id="content">
        <%= markdown @project.description%>
        <div class="button_wrapper">
          <a href='<%= @project.link%>' class='project_button'
            target="_blank">Visit <%= @project.title %></a>
        </div>
        <br>
        <br>
        <% if model_signed_in? %>
          <div id='admin_links'>
            <%= link_to "Edit", edit_post_path(@post) %>
            <%= link_to "Delete", post_path, method: :delete, data: {confirm: "Are you sure?"} %>
          </div>
        <% end %>

      </div>
    </div>
